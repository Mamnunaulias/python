#operTOR
#operator aritmatika
a=10
b=2
hasil= a+b
print(a,"+",b,"=",hasil)
hasil= a-b
print(a,'-',b,'=',hasil)
hasil=a*b
print(a,'*',b,'=',hasil)
hasil=a/b
print(a,'/',b,'=',hasil)
hasil=a**b
print(a,'**',b,'=',hasil)
hasil=a%b
print(a,'%',b,'=',hasil)
hasil=a//b
print(a,'//',b,'=',hasil)

