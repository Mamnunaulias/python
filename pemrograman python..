#operasi logika
h=10
i=11
j= h and i
print (h,'and',i,'=',j)
#or(jika slah satunya true, maka hasilnya adalah true)
a= True
b= False
c= a or b
print(a,'OR',b,'=',c)
#not
d= True
f= not d
print('data d=',d)

#percabangan
angka=6
if angka >6:
    print(angka,"bilangan genap:")
elif angka==6:
    print(angka,"bilangan not:")
else:
    print(angka,"bilangan ganjil:")

